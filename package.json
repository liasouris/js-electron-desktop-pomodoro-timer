{
  "name": "pomodoro-timer",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-win": "electron-packager . pomodoro-timer --platform=win32 --arch=x64 --out=dist/ --overwrite"
  },
  "build": {
    "appId": "com.liade.pomodoro-timer",
    "productName": "Pomodoro Timer",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!node_modules/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "description": "",
  "devDependencies": {
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12"
  }
}